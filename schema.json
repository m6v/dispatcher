{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "id": {"type": "integer"},
        "name": {"type": "string", "pattern": "^[a-zA-Z ]*$", "minLength": 5, "maxLength": 25},
        "age": {"type": "integer", "minimum": 18, "maximum": 120}
    },
    "additionalProperties": false,
    "required": ["id", "name", "age"]
}
